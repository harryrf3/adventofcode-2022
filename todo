// part one

// create max, set to 0
// loop through each element (elf) of each array (elves)
      // create a total for each elf's calories carried, set to 0.
      // for loop of calories of every elf, trimmed of whitespace and split at return '\r\n'.
        // total gets added to number of calories


    // max = Math.max(max, total)

// log the final value of 'max'

// part 2

const totals = [];
for (const elf of elves) {
    let total = 0;
    for (const calories of elf.trim().split('\r\n')) {
        total += Number(calories);
    }

    totals.push(total);
}

totals.sort((a, b) => b - a);

console.log('Part 2:', totals[0] + totals[1] + totals[2]);